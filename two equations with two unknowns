import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        System.out.println("enter the coefficient of x1");
        int a = scn.nextInt();
        System.out.println("enter the coefficient of y1");
        int b = scn.nextInt();
        System.out.println("enter the coefficient of x2");
        int d = scn.nextInt();
        System.out.println("enter the coefficient of y2");
        int e = scn.nextInt();
        System.out.println("enter the constant of the first equation");
        int c = scn.nextInt();
        System.out.println("enter the constant of the second equation");
        int f = scn.nextInt();
        int x=(c-b*f/e)/(a-b*d/e);
        int y=(c-a*x)/b;
        System.out.println("x "+x+" y "+y);
    }
}
